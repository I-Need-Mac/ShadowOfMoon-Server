{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"],
      "@common/*": ["./src/common/*"],
      "@errors/*": ["./src/commons/errors/*"],
      "@dto/*": ["./src/common/dto/*"],
      "@utils/*": ["./src/common/utils/*"],
      "@modules/*": ["./src/modules/*"],
      "@middlewares/*": ["./src/common/middlewares/*"],
      "@assets/*": ["./src/modules/assets/*"],
      "@characters/*": ["./src/modules/characters/*"],
      "@clauses/*": ["./src/modules/clauses/*"],
      "@medicines/*": ["./src/modules/medicines/*"],
      "@presets/*": ["./src/modules/presets/*"],
      "@reward_boxes/*": ["./src/modules/reward_boxes/*"],
      "@saint_souls/*": ["./src/modules/saint_souls/*"],
      "@souls/*": ["./src/modules/souls/*"],
      "@stages/*": ["./src/modules/stages/*"],
      "@users/*": ["./src/modules/users/*"]
    },
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false
  }
}
